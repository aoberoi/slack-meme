{
  "name": "Slack Memes",
  "description": "Post memes to any of your Slack channels with a slash command.",
  "repository": "https://github.com/nicolewhite/slackbot-meme",
  "keywords": ["slack", "meme"],
  "env": {
    "SLACK_API_TOKEN": {
      "description": "Your Slack App's API token.",
      "value": "CHANGE_ME"
    },
    "SLACK_WEBHOOK_URL": {
      "description": "The URL of Slack App's Incoming Webhook.",
      "value": "CHANGE_ME"
    },
    "SLACK_VERIFICATION_TOKEN": {
      "description": "Your Slack App's verification token.",
      "value": "CHANGE_ME"
    }
  }
}
